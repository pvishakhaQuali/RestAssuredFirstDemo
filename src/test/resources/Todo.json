{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "userId": {
      "type": "integer"
    },
    "id": {
      "type": "integer",
	  "minimum":1,
	  "maximum":5,
	  "exclusiveMaximum":false
    },
    "title": {
      "type": "string",
	  "minLength":2,
	  "maxLength":100
    },
    "completed": {
      "type": "boolean"
    }
  },
  "required": [
    "userId",
    "id",
    "title",
    "completed"
  ]
  ,
  "additionalProperties": true
}